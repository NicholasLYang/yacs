  "compilerOptions": {
    "outDir": "./dist-web/public/out-tsc",
    "baseUrl": "src",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "keyofStringsOnly": true,
